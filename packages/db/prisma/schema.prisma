// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  //TODO: Need Env configuration when going in production
  url      = "mongodb://localhost:27017/excluence"
}

model NFTCollection {
  id String @id @default(auto()) @map("_id") @db.ObjectId 
  externalURLSegment String @unique
  address String
  contractName String
} 
