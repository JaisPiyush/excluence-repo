{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:backend": "dotenv -- turbo run dev --filter=backend",
    "dev:frontend": "dotenv -- turbo run dev --filter=frontend",
    "sb:frontend": "yarn workspace frontend run storybook",
    "db:push": "dotenv -- yarn workspace @excluence-repo/db run db:push"
  },
  "devDependencies": {
    "@thirdweb-dev/cli": "^0.6.15",
    "@turbo/gen": "^1.9.7",
    "dotenv-cli": "^7.2.1",
    "eslint-config-custom": "*",
    "jest": "29.5.0",
    "prettier": "^2.5.1",
    "ts-node": "10.9.1",
    "turbo": "^1.9.9"
  },
  "name": "excluence-repo",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "eslint": "^8.43.0"
  }
}
